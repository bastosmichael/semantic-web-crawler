var IS_IE = $.browser.msie;
var IEdebug = false;
if (IS_IE && IEdebug) {
    $('body').append("<div id='IEconsole' style='position:absolute; z-index: 1000; top:580px; right:0; width:200px; background:#EECCCC;'>IE Debug Console -v " + IEdebug + "<\/div>");
}
this.echo = function (msg, clear) {
    var curContent = $('#IEconsole').html() + "<br />";
    if (clear) {
        curContent = '';
    }
    $('#IEconsole').html(curContent + msg);
}
jQuery.fn.extend({
    isAboveViewport: function (offset) {
        var t = $(window).scrollTop();
        return t >= this.offset().top + this.height() + (offset || 0);
    },
    isBelowViewport: function (offset) {
        var b = $(window).height() + $(window).scrollTop();
        return b <= this.offset().top + (-offset || 0);
    },
    isInViewport: function (padding) {
        return !this.isAboveViewport(padding) && !this.isBelowViewport(padding);
    }
});
var gridswitcher = (function (root, hoverLi) {
    var LOADING_TEXT = "loading latest headlines ...";
    var CLASS_LOADING = "loading";
    var HOVER_DELAY = 250;
    var currGridId = 0;
    var gridSlot = $("#upscroll-grid");
    var sections = $('#sections');
    var loaded = false;
    var loadingIndicator = null;
    var data = null;
    var currentIndex = 1;
    var videoIconHtml = '<span class="icon"><span class="video"></span></span>';
    var photoIconHtml = '<span class="icon"><span class="photo"></span></span>';

    function init() {
        if (!gridSlot.length) {
            return;
        }
        currGridId = 0;
        if (hoverLi) {
            currGridId = getGridId($(hoverLi));
        }
        if (currGridId == 0) {
            currGridId = getGridId(sections.find("li:first"));
        }
        getData(currGridId, function (d) {
            data = d;
            gridSlot.html(data);
            addMultimediaIcon();
            loaded = true;
        });
        setupBehavior();
    };

    function setupBehavior() {
        sections.find("li").each(function (index) {
            var li = $(this);
            var gid = getGridId(li);
            var delay;
            $(this).bind("mouseover", function () {
                delay = setTimeout(function () {
                    root.removeClass('s' + currentIndex);
                    handleSectionTrigger(li, gid, index);
                }, HOVER_DELAY);
            }).bind("mouseout", function () {
                clearTimeout(delay);
            });
        });
    };

    function handleSectionTrigger(li, gridId, index) {
        if (!gridId || gridId === currGridId) {
            return;
        }
        index = index + 1;
        currGridId = gridId;
        root.addClass('s' + index);
        currentIndex = index;
        getData(gridId, function (d) {
            data = d;
            gridSlot.html(data);
            addMultimediaIcon();
        });
    };

    function addMultimediaIcon() {
        var upscrollGrid = $('#upscroll-grid');
        var videoLinks = $('.story h6 a[href*=vPlayer], .story h6 a[href*=/vp/]', upscrollGrid);
        for (var i = 0; i < videoLinks.length; i++) {
            $(videoLinks[i]).prepend(videoIconHtml);
        };
        var videoLinksH4 = $('.story h4 a[href*=vPlayer], .story h4 a[href*=/vp/]', upscrollGrid);
        for (var i = 0; i < videoLinksH4.length; i++) {
            $(videoLinksH4[i]).parent().next().prepend(videoIconHtml);
        };
        var slideShowLinks = $('.story h6 a[href*=SSOpen]', upscrollGrid);
        for (var i = 0; i < slideShowLinks.length; i++) {
            $(slideShowLinks[i]).prepend(photoIconHtml);
        };
        var slideShowLinksH4 = $('.story h4 a[href*=SSOpen]', upscrollGrid);
        for (var i = 0; i < slideShowLinksH4.length; i++) {
            $(slideShowLinksH4[i]).parent().next().prepend(photoIconHtml);
        };
    }

    function getGridId(el) {
        return parseInt(el.attr('grid'), 10);
    };

    function getData(gridId, callback) {
        var self = this;
        var url = String(window.location.href).split(".com/")[0] + ".com/id/" + gridId;
        jQuery.ajax({
            "dataType": "html",
            "url": url,
            "success": function (data) {
                if (data && callback) {
                    callback.call(self, data);
                }
            }
        });
    };
    init();
    return {};
});
var upscroll = (function () {
    var ID_NET = "network";
    var ID_SITEHEADER = "siteHeader";
    var ID_NAV = "site";
    var ID_MARQUEE = "marqueeContainer";
    var CLASS_MAIN = ".main";
    var ACTION_HOVER = "hover";
    var ACTION_SCROLL = "scroll";
    var CLASS_SELECTED = "selected";
    var GRID_HEIGHT = 185;
    var TODAY_GRID_HEIGHT = 210;
    var HOVER_ON_NAV_ITEM = 500;
    var HOVER_OPEN_SPEED = 300;
    var SCROLLTOP_WHERE_75_PERCENT_IN_VIEW = 100;
    var ID_LIST = "sections";
    var IS_TODAY_SHOW = false;
    var ATTR_GRID_ID = "data-grid-id";
    var ID_COMCAST = "xfinity";
    var MY = {};
    MY.init = function (jq_rootElement, type, data) {
        var breadcrumb = $("#breadcrumbs .breadcrumbs li");
        for (var i = 0; i < breadcrumb.length; i++) {
            if (i == breadcrumb.length - 1) {
                $(breadcrumb[i]).css({
                    "display": "inline"
                });
            } else {
                $(breadcrumb[i]).css({
                    "display": "none"
                });
            }
        };
        $('#todayShow #breadcrumbs').contents().filter(function () {
            return this.nodeType == 3;
        }).remove();
        if ("undefined" === typeof document.body.style.maxHeight) {
            return;
        }

        this.cookie = readCookie('P7');
        if (this.cookie != null && this.cookie === 'X') {
            this.isComcast = true;
            this.comcastHeight = 44;
        }
        else {
            this.isComcast = false;
            this.comcastHeight = 0;
        }

        var NAV_CONTENT_ADDITIONAL_HEIGHT = 63 + 212;
        this.root = $(jq_rootElement);
        this.net = $("#" + ID_NET);
        this.nav = $("#" + ID_NAV);
        this.list = $("#" + ID_LIST);
        this.siteHeader = $("#" + ID_SITEHEADER);
        this.marquee = $("#" + ID_MARQUEE);
        this.sections = $("#sections li");
        this.relLayers = this.root.find(".layer");
        this.navContent = this.nav.find(CLASS_MAIN);
        this.grid = this.navContent.find("#upscroll-grid");
        this.netHeight = this.net.height() + 1;
        this.navHeight = this.nav.height();
        this.listHeight = this.list.height();
        this.marqueeHeight = this.marquee.height();
        this.navContentHeight = this.navContent.height();
        this.fixedElems = this.netHeight + this.marqueeHeight + this.listHeight;
        this.hdrHeight = this.navContentHeight + this.fixedElems + NAV_CONTENT_ADDITIONAL_HEIGHT - 35;
        this.defHeight = this.navHeight + this.fixedElems;
        this.upHeight = this.hdrHeight - this.defHeight;
        this.netIsFixed = false;
        this.hasExtendedOnce = false;
        this.notFullyExtended = false;
        this.isUpscrollOpen = false;
        this.switcher = null;
        this.currLi = null;
        if (this.isComcast == true) {
            this.comcast = $("#" + ID_COMCAST);
        }
  this.checkComcast();
 };
 MY.isComcastLoaded = function(){
   var self = this;
         setTimeout(function () {
             self.comcast = $("#xfinity");
             self.checkComcast();
         }, 500);
  };
  MY.checkComcast = function(){
   if (this.isComcast === true) {
          if (this.comcast.length === 0) {
              this.isComcastLoaded();
          }
          else {
    //faking bg for quirks

   if(IS_IE){
        this.comcast.wrap('<div id="xfinity_bg"/>');
     }
              var self = this;
              $(window).resize(function () {
                  self.sizeXfinity();
              });

              this.sizeXfinity();
              this.start();
          }
      }
      else {
          this.start();
      }

  };
  MY.sizeXfinity = function(){
 //faking bg for quirks

   if(IS_IE){
  var margin = ($(window).width()-1100)/2;
  this.comcast.css({'width':'1100px', 'margin-left': margin});
 }
 else{
  this.comcast.css({ 'width': $(window).width() });  
 }

  };
 MY.start = function(){
        this.reset();
        if (!IS_IE) {
            $("#siteHeader .main #upscroll-grid").css({
                "height": "185px"
            });
            this.reset();
            if (this.root.isInViewport()) {
                $("html,body").scrollTop(this.upHeight);
            }
            this.bindEvents();
        } else {
            var self = this;
            $(window).scrollTop(5);
            $(window).scroll(function (e) {
                if ($(window).scrollTop() == 0 && !this.hasExtendedOnce) {
                    self.siteHeader.css({
                        "height": "307px"
                    });
                    self.nav.css({
                        "height": "250px"
                    });
                    $("#siteHeader .main #upscroll-grid").css({
                        "height": "185px",
                        "margin-bottom": "10px"
                    });
                    $("#tbx-37650045").css({
                        "zoom": "1"
                    });
                    $("#siteHeader #coverbadge #wx-badge").css({
                        "margin": "10px 0 -10px",
                        "zoom": "1",
                        "clear": "both"
                    });
                    this.hasExtendedOnce = true;
                    if (!self.isUpscrollOpen) {
                        self.isUpscrollOpen = true;
                        if (!self.currLi) {
                            self.currLi = self.sections[0];
                        }
                        $(self.currLi).addClass(CLASS_SELECTED);
                    }
                    var switcher = gridswitcher(self.navContent, self.currLi);
                    self.grid.addClass('active');
                }
            });
            this.bindEventsForIE();
            $("#msn .more").mouseover(function (e) {
                $(this).addClass("moreOut");
            });
            $("#msn .more").mouseout(function (e) {
                $(this).removeClass("moreOut");
            });
        }
    };
    MY.reset = function () {
        if (this.isComcast === true) {
            this.comcast.css({ "top": this.upHeight, "position": "absolute" }); //new line
            this.sizeXfinity();
        } 
        this.root.height(this.hdrHeight + 8);
        this.nav.css({
            "top": this.upHeight,
            "height": this.navHeight,
            "overflow": "visible"
        });
        this.relLayers.css({
            "top": this.upHeight,
            "position": "relative",
            "z-index": 1200
        });
    this.net.css({
     "position": "relative",
     "z-index": 1300
    });
        this.navContent.css("top", this.upHeight * -1);
        this.grid.find('.grid').css({
            "display": "none"
        });
        this.netIsFixed = false;
        if (IS_IE) {
         if (this.isComcast === true) {
             this.comcast.css({ "top": 0, "position": "relative" }); //new line
             this.sizeXfinity();
         }
            this.root.height(this.hdrHeight - 250);
            this.net.css({
                "top": 0,
                "position": "relative",
                "left": 0
            });
            this.marquee.css({
                "top": 0,
                "position": "relative",
                "left": 0
            });
            this.list.css({
                "top": 0,
                "position": "relative"
            });
            this.navContent.css("top", 0);
            this.nav.css("top", 0);
            echo("reset()");
        } else {
            this.grid.css({
                "height": GRID_HEIGHT
            });
        }
        if (this.isUpscrollOpen) {
            this.isUpscrollOpen = false;
        }
    };
    MY.update = function (action_type, li) {
        var scrollTop = $(window).scrollTop();
        if (scrollTop > this.upHeight) {
            if (this.netIsFixed) {
                this.reset();
                this.grid.css({
                    "visibility": "hidden"
                });
            }
            return;
        }
        if (scrollTop != 0) {
            this.sections.removeClass(CLASS_SELECTED);
        } else if (this.currLi) {
            $(this.currLi).addClass(CLASS_SELECTED);
        }
        if (this.netIsFixed == false || !this.hasExtendedOnce) {
            if (this.isComcast === true) {
                this.root.css({ "top": this.comcastHeight, "margin-bottom": this.comcastHeight });
                this.comcast.css({ "top": 0, "position": "fixed", "left": 0, "z-index": 1500 }); //new line
            }
   this.net.css({
                "top": this.comcastHeight,
                "position": "fixed",
                "left": 0
            });
            this.marquee.css({
                "top": this.netHeight + this.comcastHeight,
                "position": "fixed"
            });
            this.list.css({
                "top": this.netHeight + this.marqueeHeight + this.comcastHeight,
                "position": "fixed"
            });
            this.nav.css({
                "overflow": "hidden"
            });
            this.root.height(this.hdrHeight);
            if (!this.hasExtendedOnce) {
                this.grid.addClass('active');
            } else {
                this.grid.css({
                    "visibility": "visible"
                });
            }
            this.grid.find(".grid").css({
                "display": "block"
            });
            this.netIsFixed = true;
        }
        this.nav.css({
            "height": this.hdrHeight - scrollTop - this.fixedElems,
            "top": scrollTop + this.fixedElems
        });
        this.navContent.css("top", scrollTop * -1);
        if (action_type == ACTION_HOVER) {
            this.navContent.css("top", 0);
            $("html, body").animate({
                scrollTop: 0
            }, HOVER_OPEN_SPEED);
        }
        if (scrollTop < SCROLLTOP_WHERE_75_PERCENT_IN_VIEW || action_type == ACTION_HOVER) {
            if (!this.isUpscrollOpen) {
                this.isUpscrollOpen = true;
                if (!this.currLi) {
                    this.currLi = this.sections[0];
                }
                $(this.currLi).addClass(CLASS_SELECTED);
            }
            if (!this.hasExtendedOnce) {
                this.initialUpdate(action_type, this.currLi);
            }
        }
    };
    MY.initialUpdate = function (action_type, li) {
        var self = this;
        var scrollTop = $(window).scrollTop();
        this.hasExtendedOnce = true;
        this.grid.css({
            "visibility": "visible"
        });
        this.switcher = gridswitcher(this.navContent, li);
    };
    MY.bindEvents = function () {
        var self = this;
        var doCheck = true;
        var delay;
        var exposedHover = HOVER_ON_NAV_ITEM;
        this.list.find("a").bind("mouseover", function () {
            var hoverLi = $(this).parent();
            var currGridID = self.getGridID(hoverLi);
            if (currGridID == 0) {
                return;
            }
            if (hoverLi != self.currLi) {
                self.sections.removeClass(CLASS_SELECTED);
                self.currLi = hoverLi;
            }
            if ($(window).scrollTop() == 0) {
                hoverLi.addClass(CLASS_SELECTED);
            }
            delay = setTimeout(function () {
                if (!self.hasExtendedOnce) {
                    self.update(ACTION_HOVER, hoverLi);
                }
                if ($(window).scrollTop() < self.upHeight) {
                    exposedHover = 100;
                    self.update(ACTION_HOVER, hoverLi);
                }
            }, exposedHover);
        }).bind("mouseout", function () {
            clearTimeout(delay);
        });
        $(window).scroll(function (e) {
            self.update(ACTION_SCROLL);
            doCheck = false;
        });
        setTimeout(function () {
            self.update(ACTION_SCROLL);
            if (self.root.isInViewport()) {
                $("html,body").scrollTop(self.upHeight);
            }
        }, 500);
    };
    MY.bindEventsForIE = function () {
        var self = this;
        var doCheck = true;
        var delay;
        var exposedHover = HOVER_ON_NAV_ITEM;
        this.list.find("a").bind("mouseover", function () {
            var hoverLi = $(this).parent();
            var currGridID = self.getGridID(hoverLi);
            if (currGridID == 0) {
                return;
            }
            if (hoverLi != self.currLi) {
                self.sections.removeClass(CLASS_SELECTED);
                self.currLi = hoverLi;
            }
            if ($(window).scrollTop() == 0) {
                hoverLi.addClass(CLASS_SELECTED);
            }
            delay = setTimeout(function () {
                $(window).scrollTop(0);
            }, exposedHover);
        }).bind("mouseout", function () {
            clearTimeout(delay);
        });
    };
    MY.getGridID = function (el) {
        return parseInt(el.attr('grid'), 10);
    };
    return MY;
})();
$(document).ready(function () {
 upscroll.init($("#siteHeader"));
});
try {
    var AdBreakingNews = {};
    AdBreakingNews.Render = function () {
        try {
            if (AdBreakingNews.Enable) {
                $("div#marquee").addClass("adBnm");
                $("div#marquee").prepend('<div id="AdBnmRight"></div><div id="AdBnmLeft"></div>');
                if (AdBreakingNews.Left && AdBreakingNews.Left != "") {
                    dapMgr.enableACB("AdBnmLeft", false);
                    dapMgr.renderAd("AdBnmLeft", AdBreakingNews.Left, 88, 31);
                    AdBreakingNews.Rendered = true
                }
                if (AdBreakingNews.Right && AdBreakingNews.Right != "") {
                    dapMgr.enableACB("AdBnmRight", false);
                    dapMgr.renderAd("AdBnmRight", AdBreakingNews.Right, 88, 31);
                    AdBreakingNews.Rendered = true
                }
                $(window).resize(function () {
                    $(window).width() > 1158 ? $("div#marquee").addClass("adBnm") : $("div#marquee").removeClass("adBnm")
                })
            }
        } catch (a) {}
    };
    AdBreakingNews.InitAds = function () {
        AdBreakingNews.Left = "";
        AdBreakingNews.Right = "";
        AdBreakingNews.Width = 1024;
        AdBreakingNews.Enable = false;
        AdBreakingNews.Rendered = false;
        AdBreakingNews.Url = "/xml.aspx?id=39654701";
        $.get(AdBreakingNews.Url, function (a) {
            $(a).find("DisplayAd").each(function () {
                if ($(this).find("Enabled").text().toLowerCase() === "true") {
                    var d = $(this).find("AdLocation").text(),
                        b = $(this).find("Content").text(),
                        c = $(this).find("AdPlacementCode").text();
                    switch (d) {
                    case "AdBreakingNewsMarqueeLeft":
                        AdBreakingNews.Left = "&PG=" + b + "&AP=" + c;
                        AdBreakingNews.Enable = true;
                        break;
                    case "AdBreakingNewsMarqueeRight":
                        AdBreakingNews.Right = "&PG=" + b + "&AP=" + c;
                        AdBreakingNews.Enable = true
                    }
                }
            });
            if (AdBreakingNews.Enable && $(window).width() > AdBreakingNews.Width) AdBreakingNews.Render();
            else AdBreakingNews.Enable && $(window).resize(function () {
                AdBreakingNews.Enable && AdBreakingNews.Rendered === false && $(window).width() > AdBreakingNews.Width && AdBreakingNews.Render()
            })
        })
    }
