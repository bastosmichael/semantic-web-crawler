<script>

	var oBoxes = $('#homeDepotBoxOrder').val();
	
	if(oBoxes != ''){	
		var oBoxesArr = oBoxes.split('&');
		var depotHtml = '';
		for(var i=0; i<oBoxesArr.length; i++){
			if(i == 0){
				
				depotHtml = depotHtml + "<div id='" + oBoxesArr[i] + "'class='oCol1of4 first depot'>" + $('#' + oBoxesArr[i]).html() + "</div>";
			}
			else {
				depotHtml = depotHtml + "<div id='" + oBoxesArr[i] + "'class='oCol1of4 depot'>" + $('#' + oBoxesArr[i]).html() + "</div>";
			}
		}
		$('#depotBoxes').html(depotHtml);
	}	
	
</script>



				
			
			
			
			
				
				
				
